    <script>
const Array = [
  [1,2,3,4,5],
  [7,8,9,10],
  [11,12,13],
]
const Array2 = [
  [4,5,6],
  [10,13],
  [20,23],
]

function myArray(arr){
  let randoms = [];
    for(let r = 0; r < arr.length; r++){
        let ran = 0;
    for (let n = 0; n < arr[r].length; n++){
      ran += arr[r][n];
    }
    randoms.push(ran);
    }
    return randoms;
}
</script>
